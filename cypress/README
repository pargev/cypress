cd PATH
npm install

to open Cypress
./node_modules/cypress/bin/cypress open

./node_modules/cypress/bin/cypress --browser chrome --reporter mochawesome --headed

./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/Github.js  --env grep=Pull
./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/Github.js  --env grep=Marketplace
./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/Github.js  --env grep=Explore
./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/Github.js  --env grep=Issues

./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/Github.js  --env grep=Issues --env username=USERNAME --env password=PASSWORD

./node_modules/cypress/bin/cypress run --browser chrome --reporter mochawesome --headed --spec=cypress/integration/tests/GithubNavigation.js  --env grep=Issues --env username=USERNAME --env password=PASSWORD

